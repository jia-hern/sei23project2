<div>
    <% orders.forEach ((order, count) => {%>
    <table>
        <tr>Order <%= count +1 %></tr>
        <tr>
            <td>Item name</td>
            <td>Quantity</td>
            <td>Price</td>
        </tr>
        <% order.items.forEach(item => { %>
        <tr>
            <td><%= item.item.name %></td>
            <td><%= item.quantity %></td>
            <td><%= item.item.price %></td>
        </tr>
        <% }) %>
    </table>
    <% }) %>
</div>